   "source": [
    "def shorten(text):\n",
    "    return ''.join(word[0] for word in text.split()).upper()\n",
    " \n",
    "if __name__ == '__main__':\n",
    "    shortened = shorten(\"Dont me\")\n",
    "    print(shortened)\n",
    "    "
